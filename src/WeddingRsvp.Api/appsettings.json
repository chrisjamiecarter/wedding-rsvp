{
  "AllowedHosts": "*",
  "Authentication": {
    "Google": {
      "ClientId": "your_google_client_id",
      "ClientSecret": "your_google_client_secret"
    }
  },
  "ConnectionStrings": {
    "WeddingRsvpConnection": "your-connection-string"
  },
  "JwtOptions": {
    "Audience": "your-jwt-audience",
    "Issuer": "your-jwt-issuer",
    "Secret": "your-jwt-secret",
    "TokenLifetimeInHours": 8
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SeederOptions": {
    "SeedDatabase": true,
    "SeedUser": {
      "Email": "admin@email.com"
    }
  }
}
